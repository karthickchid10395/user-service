		{
			"name": "User Registration - Success Scenarios",
			"item": [
				{
					"name": "Valid User Registration",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has success message\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.message).to.eql(\"User created successfully\");",
									"});",
									"",
									"pm.test(\"No error info present\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.errorInfo).to.be.oneOf([null, undefined]);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"firstname\": \"John\",\n  \"lastname\": \"Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"username\": \"johndoe123\",\n  \"password\": \"SecurePass@123\",\n  \"confirmpassword\": \"SecurePass@123\",\n  \"countrycode\": \"+1\",\n  \"mobilenumber\": \"9876543210\"\n}"
